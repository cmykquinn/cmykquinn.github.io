  <link rel="preconnect" href="https://rsms.me/" /><link
    rel="stylesheet"
    href="https://rsms.me/inter/inter.css"
  />
  <link rel="icon" href="/favicon.svg" type="image/svg+xml" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  <body>
    <ul class="flex items-center pt-5">
      <li class="px-4"><a href="/">Home</a></li>
      <li class="px-4"><a href="/blog">Blog</a></li>
      <li class="px-4"><a href="/projects">Projects</a></li>
      <li class="px-4"><a href="/me">About</a></li>
      <li id="date-time" class="text-right font-bold ml-auto px-4"></li>
    </ul>
    <h1 class="text-4xl font-bold underline pt-10 pl-10 sm:pl-5">cmykquinn</h1>
    <div class="flex pl-10 py-3 sm:pl-5">
      <Image
        src="https://i.imgur.com/10ttDG2.png"
